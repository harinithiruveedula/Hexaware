                <then>
                    <if expression="num1&gt;num3">
                        <then>
                            <output expression="num1 &amp; &quot; &quot; &amp; &quot;is the largest number&quot;" newline="True"/>
                        </then>
                        <else/>
                    </if>
                </then>
                <else>
                    <if expression="num2&gt;num3">
                        <then>
                            <output expression="num2 &amp; &quot; &quot; &amp; &quot;is the largest&quot;" newline="True"/>
                        </then>
                        <else>
                            <output expression="num3&amp;&quot; &quot;&amp; &quot;is the largest&quot;" newline="True"/>
                        </else>
                    </if>
                </else>
            </if>
        </body>
