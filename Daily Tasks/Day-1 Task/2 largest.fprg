    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <comment text="largest of three numbers"/>
            <declare name="num1, num2, num3" type="Integer" array="False" size=""/>
            <output expression="&quot;enter the num1&quot;" newline="True"/>
            <input variable="num1"/>
            <output expression="&quot;enter num2 &quot;" newline="True"/>
            <input variable="num2"/>
            <output expression="&quot;enter num 3&quot;" newline="True"/>
            <input variable="num3"/>
